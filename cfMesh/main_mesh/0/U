FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (1 0 0);

boundaryField
{
       front
    {
        type            symmetry;
    }
    back
    {
        type            symmetry;
    }
       
   
       top
    {
        type            symmetry;
    }
    bottom
    {
        type            noSlip;
    }
    airfoil
    {
        type            noSlip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    outlet
    {
        type            zeroGradient;

    }
}


