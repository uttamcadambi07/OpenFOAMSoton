FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 2.5e-06;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient; //fixedValue;
        //value           $internalField;
    }

    topWall
    {
        type            symmetry;

    }

    bottomWall
    {

        type            nutLowReWallFunction	;
        value           uniform 0;
    }

    frontAndBack
    {
        type            symmetry;
    }
}


